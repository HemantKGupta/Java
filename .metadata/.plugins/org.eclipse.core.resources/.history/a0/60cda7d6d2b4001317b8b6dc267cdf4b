import java.util.ArrayList;
import java.util.List;


public class BasicListOperationsTest {

	public static void main(String[] args) {
		
		List<String> al = new ArrayList<String>();
        al.add("JAVA");
        al.add("C++");
        al.add("PERL");
        al.add("PHP");
        al.add(0, "Ruby");
        al.remove(3);
        al.remove("PHP");
	}

}
