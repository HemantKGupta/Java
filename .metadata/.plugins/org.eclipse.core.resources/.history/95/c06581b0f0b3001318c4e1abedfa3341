public class Foo {
   		static int x=7;
    	static void change(int x){
    		x=x+50;
    		System.out.println("x inside change is  :"+x);
    	}
    	
    	static void change2(int x){
    		Foo.x=x+50;
    		System.out.println("x inside change is  :"+Foo.x);
    	}
        public static void main(String[] args) {
        change(2);
        System.out.println("x is :"+x);
        change2(2);
        System.out.println("x is :"+x);
    }
}
